[
  {
    "name": "App Store Receipts",
    "description": "Organizes App Store/iCloud receipts by year",
    "conditions": {
      "extension": "pdf",
      "content_contains": ["receipt", "icloud"]
    },
    "actions": {
      "rename_pattern": "App Store_{date}_{year}{ext}",
      "move_to": "/Users/chadmiller/Documents/Filing Cabinet/{year}/Receipts"
    }
  },
  {
